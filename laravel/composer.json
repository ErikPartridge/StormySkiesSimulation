{
	"name": "erikmp/takeoffsim",
	"description": "An Airline Simulation",
	"keywords": ["airline", "laravel"],
	"license": "Copyright Erik Partridge",
	"repositories": [
    {
        "type":"package",
        "package": {
          "name": "erikmp/vanilla",
          "version":"dev-master",
          "source": {
              "url": "https://github.com/erikmp/laravel-vanilla-integration",
              "type": "git",
              "reference":"master"
            }
        }
    }
	],
	"require": {
        "php": ">= 5.4.0",
		"laravel/framework": "4.2.*",
        "league/geotools": "@stable",
        "fzaninotto/faker": "1.5.*@dev",
        "laravel/cashier": "~2.0",
        "cartalyst/sentry": "2.1.*",
        "guzzlehttp/guzzle": "~4.0",
		"bogardo/mailgun": "3.0.*",
		"erikmp/vanilla": "dev-master"
	},
	"require-dev": {
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/libraries"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
